generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TokenState {
  id           Int      @id @default(autoincrement())
  timestamp    DateTime @default(now())
  tokenName    String
  tokenAddress String
  blockNumber  String
  eventIndex   Int
  supply       String
  price     String
  marketCap    String
  txHash       String?

  @@unique([tokenAddress, blockNumber, eventIndex])
  @@index([tokenAddress])
}